{
    "name": "funcai/funcai-php",
    "description": "High performance, state of the art machine learning in php",
    "type": "library",
    "keywords": [
      "tensorflow",
      "ai",
      "machine learning",
      "image recognition",
      "prediction",
      "ffi"
    ],
    "license": "MIT",
    "require": {
      "php": "^7.4.0|^8.0",
      "ext-gd": "*",
      "ext-curl": "*",
      "ext-phar": "*",
      "ext-ffi": "*",
      "ext-json": "*"
    },
    "autoload": {
      "psr-4": {
        "FuncAI\\": "src/",
        "Tests\\": "tests/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12.98",
        "friendsofphp/php-cs-fixer": "^3.1"
    }
}
